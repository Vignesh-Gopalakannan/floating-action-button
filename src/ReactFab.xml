<?xml version="1.0" encoding="utf-8"?>
<widget id="mxtechies.reactfab.ReactFab" pluginWidget="true" needsEntityContext="true" offlineCapable="true" supportedPlatform="Web"
    xmlns="http://www.mendix.com/widget/1.0/"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.mendix.com/widget/1.0/ ../node_modules/mendix/custom_widget.xsd">
    <name>React FAB</name>
    <description>React floating action buttons</description>
    <icon></icon>
    <properties>
        <propertyGroup caption="General">
            <property key="mainIcon" type="icon">
                <caption>Main Icon</caption>
                <description>Main button icon</description>
            </property>
            <property key="mainIconText" type="string" required="false">
                <caption>Main Icon Text</caption>
                <description>Text to be displayed on interaction with main icon</description>
            </property>
            <property key="event" type="enumeration" defaultValue="hover">
                <caption>Event</caption>
                <description>Event type to display the action buttons</description>
                <enumerationValues>
                    <enumerationValue key="hover">Hover</enumerationValue>
                    <enumerationValue key="click">Click</enumerationValue>
                </enumerationValues>
            </property>
            <property key="object" type="object" isList="true">
                <caption>Action Button List</caption>
                <description/>
                <properties>
                    <propertyGroup caption="Action Button">
                        <property key="iconText" type="string">
                            <caption>Icon Text</caption>
                            <description>Text to be displayed on interaction with action icon</description>
                        </property>
                        <property key="buttonIcon" type="icon">
                            <caption>Icon</caption>
                            <description>Action button icon</description>
                        </property>
                        <property key="objectAction" type="action">
                            <caption>Action</caption>
                            <description>Action to be triggered on click of action button</description>
                        </property>
                    </propertyGroup>
                </properties>
            </property>
        </propertyGroup>
        <propertyGroup caption="Styles">
            <property key="mainBg" type="string">
                <caption>Main Icon BG</caption>
                <description>Background color of the main icon in Hex code (#E74C3C)</description>
            </property>
            <property key="mainColor" type="string">
                <caption>Main Icon Color</caption>
                <description>Color of the main icon in Hex code (#FFFFFF)</description>
            </property>
            <property key="actionBg" type="string">
                <caption>Action Icons BG</caption>
                <description>Background color of the action icons in Hex code (#3498DB)</description>
            </property>
            <property key="actionColor" type="string">
                <caption>Action Icons Color</caption>
                <description>Color of the action icons in Hex code (#FFFFFF)</description>
            </property>
        </propertyGroup>
    </properties>
</widget>